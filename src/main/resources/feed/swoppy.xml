<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:int="http://www.springframework.org/schema/integration"
       xmlns:feed="http://www.springframework.org/schema/integration/feed"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
                            http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                            http://www.springframework.org/schema/integration
                            http://www.springframework.org/schema/integration/spring-integration.xsd
                            http://www.springframework.org/schema/integration/feed
                            http://www.springframework.org/schema/integration/feed/spring-integration-feed.xsd">

    <int:channel id="swoppyFeed">
        <int:queue/>
    </int:channel>

    <feed:inbound-channel-adapter id="feedAdapter" channel="swoppyFeed" url="http://www.musicswopshop.com.au/RSS">
        <int:poller fixed-rate="30000" max-messages-per-poll="10"/>
    </feed:inbound-channel-adapter>

</beans>